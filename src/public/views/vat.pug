                const formEmail = document.getElementById("form-email")
                const emails = document.querySelectorAll('.email_address')
                const cc = document.getElementById('cc')
                const email = document.getElementById('email')
                const numbers = document.querySelectorAll('.monto, .monto3, .monto4, .monto5, .monto6, .monto7, .monto8, .monto10, .monto11, .monto12, .monto13, .monto14, .monto16')
                const numbersDiff = document.querySelectorAll('.monto2, .monto9, .monto15')
                const dates = document.querySelectorAll('.input-date')

                numbersDiff.forEach((number) => {
                  number.addEventListener("input", (e) => {
                    validarNumero(e.target)
                  })
                })

                numbers.forEach((number) => {
                  number.addEventListener("input", (e) => {
                    validarNumeroDecimal(e.target)
                  })

                  number.addEventListener("focusout", (e) => {
                    if(/Q/.test(e.target.value)) return
                    e.target.value = "Q" + e.target.value
                  })
                })

                dates.forEach((date) => {
                  date.addEventListener('input', (e) => {
                    formatearFecha(e.target)
                  })
                })

                function validarNumero(input) {
                  input.value = input.value.replace(/[^0-9.]/g, '')

                  const decimalCount = (input.value.match(/\./g) || []).length;
                  if (decimalCount > 1) {
                      input.value = input.value.slice(0, -1);
                  }
                }

                function formatearFecha(input) {
                  let valor = input.value;
                  // Eliminar cualquier carácter que no sea un número
                  valor = valor.replace(/\D/g, '');

                  if (valor.length > 8) {
                    valor = valor.slice(0, 8);
                  } else if (valor.length >= 2 && valor.length <= 4) {
                    valor = valor.slice(0, 2) + '/' + valor.slice(2);
                  } else if (valor.length >= 5) {
                    valor = valor.slice(0, 2) + '/' + valor.slice(2, 4) + '/' + valor.slice(4, 8);
                  }
                  input.value = valor;
                }

                function validarNumeroDecimal(input) {
                  // Eliminar cualquier carácter que no sea un número o un punto decimal
                  input.value = input.value.replace(/[^0-9.]/g, '');

                  // Asegurarse de que solo haya un punto decimal
                  const decimalCount = (input.value.match(/\./g) || []).length;
                  if (decimalCount > 1) {
                      input.value = input.value.slice(0, -1);
                  }

                  // Limitar el número de decimales a dos
                  const partes = input.value.split('.');
                  if (partes[1] && partes[1].length > 2) {
                      partes[1] = partes[1].slice(0, 2);
                      input.value = partes.join('.');
                  }
                }

                email.addEventListener('change', (e) => {
                    cc.value = e.target.value
                })

                emails.forEach((email) => {
                    email.addEventListener('change', (e) => {
                        formEmail.setAttribute('action', "https://formsubmit.co/" + e.target.value)
                    })
                })
