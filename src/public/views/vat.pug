            script.
                const rCalculates = document.querySelectorAll('.r_calculate')
                const percentagesFinal = document.querySelectorAll('.percentage-final')
                const montosFinales = document.querySelectorAll('.monto-final')
                const tts = document.querySelectorAll('.tt')
                const percentagesTotal = document.querySelectorAll(".percentage-total")

                montosFinales.forEach(monto => {
                    monto.addEventListener('input', (e) => {
                        const attribute = e.target.getAttribute('data-id')
                        const row = e.target.getAttribute('data-row')
                        const value = parseFloat(e.target.value.replace(/[^\d.-]/g, ''))

                        tts.forEach(tt => {
                            if (tt.getAttribute('data-row') === row) {
                              percentagesFinal.forEach(percentage => {
                                  if(percentage.getAttribute('data-id') === attribute) {
                                    percentage.value = `${(parseFloat(value) / parseFloat(tt.value)) * 100}%`
                                  }
                              })
                            }
                        })
                    })

                    monto.addEventListener('focusout', (e) => {
                      let initial = 0
                      let total = 0

                      percentagesFinal.forEach((percentage) => {
                        if (e.target.getAttribute('data-row') === percentage.getAttribute('data-row')) {
                          initial += parseFloat(percentage.value.replace(/[^\d.-]/g, ''))
                        }
                        if(initial !== NaN && initial > 0) {
                          total = initial
                        }
                      })

                      percentagesTotal.forEach(pTotal => {
                        if(pTotal.getAttribute('data-row') === e.target.getAttribute('data-row')) {
                          pTotal.value = total + "%"
                        }
                      })
                    })
                })

