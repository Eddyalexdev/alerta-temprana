        style.
            /* Styles for the modal */
            .modal {
                display: none;
                position: fixed;
                z-index: 1;
                left: 0;
                top: 0;
                width: 100%;
                height: 100%;
                background-color: rgba(0, 0, 0, 0.5);
            }
            .input-final {
              background: none;
              outline: none;
              border: none;
            }
            .modal-content {
                background-color: #fff;
                margin: 10% auto;
                padding: 20px;
                border: 1px solid #888;
                width: 80%;
            }
            /* Styles for the form */
            form {
                display: flex;
                flex-direction: column;
            }
            label {
                font-weight: bold;
            }
            input, textarea {
                margin-bottom: 10px;
                padding: 5px;
                border: 1px solid #ccc;
            }
            /* Styles for the submit button */
            .submit-button {
                background-color: rgb(255, 175, 3);
                color: #fff;
                padding: 10px 20px;
                border: none;
                cursor: pointer;
            }
            /* Close button style */
            .close {
                color: #888;
                float: right;
                font-size: 28px;
                font-weight: bold;
            }
            .close:hover {
                color: #000;
                text-decoration: none;
                cursor: pointer;
            }
        script.
            function exportTables () {
                new Table2Excel('table').export()
            }
